{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://pedidos-backend-0e1s.onrender.com/api/$1" }
  ],
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
